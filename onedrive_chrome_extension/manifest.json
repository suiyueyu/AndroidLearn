{
	"name": "Omnibox Example",
	"description": "To use, type 'omnix' plus a search term into the Omnibox.",
	"version": "1.1",
	"content_scripts": [{
		"matches": ["https://*.livefilestore.com/*"],

		"js": ["myscript.js"]
	}],
	"permissions": [
		"*://*/*",
		// "https://*/*",
		"cookies",
		"background"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"manifest_version": 2
}