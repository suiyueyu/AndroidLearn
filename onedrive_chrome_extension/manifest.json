{
	"name": "Omnibox Example",
	"description": "To use, type 'omnix' plus a search term into the Omnibox.",
	"version": "1.1",
	"content_scripts": [{
		"matches": ["https://*.livefilestore.com/*"],
		"run_at": "document_start",
		"js": ["js/myscript.js"]
	}, {
		// "run_at":"document_start",
		"matches": ["https://*.onedrive.live.com/*"],
		"css":["css/onedriveFolder.css"],
		"js": ["lib/jquery-2.1.4.min.js", "js/onedriveFolder.js"]
	}],

	"permissions": [
		"*://*/*",
		// "https://*/*",
		"cookies",
		"background"
	],
	"background": {
		"scripts": ["js/background.js"]
	},
	"manifest_version": 2
}

